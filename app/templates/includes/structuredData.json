{
  "@context": "https://schema.org",
  "@type": "WebApplication",
  "name": "NotesReview",
  "author": "ENT8R",
  "description": "An interface for searching and resolving OpenStreetMap notes",
  "logo": "https://notesreview.org/icons/icon.png",
  "url": "https://notesreview.org/",
  "applicationCategory": "Utility",
  "applicationSubCategory": "OpenStreetMap Utility",
  "browserRequirements": "Requires JavaScript. Requires HTML5.",
  "isAccessibleForFree": true,
  "license": "https://www.gnu.org/licenses/gpl-3.0.html",
  "operatingSystem": "All",
  "releaseNotes": "https://github.com/ENT8R/NotesReview/releases",
  "softwareHelp": {
    "@type": "CreativeWork",
    "url": "https://github.com/ENT8R/NotesReview/issues"
  },
  "softwareVersion": "{{version}}",
  "screenshot": "https://cdn.jsdelivr.net/gh/ENT8R/NotesReview/assets/screenshots/light/map.png",
  "potentialAction": {
    "@type": "SearchAction",
    "target": "https://notesreview.org/?query={search_term_string}",
    "query-input": "required name=search_term_string"
  }
}
